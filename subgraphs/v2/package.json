  "repository": {
    "url": "sushiswap/subgraphs",
    "directory": "subgraphs/v2"
  },
  "files": [
    "generated"
  ],
  "scripts": {
    "generate": "mustache ../../config/$NETWORK.js template.yaml > subgraph.yaml && mustache ../../config/$NETWORK.js src/constants/index.template.ts > src/constants/index.ts && graph codegen",
    "build": "graph build",
    "test": "graph test -r",
    "create-local": "graph create --node http://localhost:8020/ sushiswap/v2-ethereum",
    "remove-local": "graph remove --node http://localhost:8020/ sushiswap/v2-ethereum",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 sushiswap/v2-ethereum",
    "deploy-server": "graph deploy --node https://graph.octodefi.com/deploy --ipfs https://ipfs.octodefi.com/ javsphere",
    "create-server": "graph create --node https://graph.octodefi.com/deploy javsphere",
    "remove-server": "graph remove --node https://graph.octodefi.com/deploy javsphere",
    "deploy-server-testnet": "graph deploy --node https://graph-testnet.octodefi.com/deploy --ipfs https://ipfs.octodefi.com/ javsphere",
    "create-server-testnet": "graph create --node https://graph-testnet.octodefi.com/deploy javsphere",
    "remove-server-testnet": "graph remove --node https://graph-testnet.octodefi.com/deploy javsphere"
  },
  "devDependencies": {
    "abi": "workspace:*",
    "@graphprotocol/graph-cli": "^0.70.0",
    "@graphprotocol/graph-ts": "^0.27.0",
    "assemblyscript": "^0.19.20",
    "matchstick-as": "0.6.0",
    "wabt": "1.0.24"
  }
}
